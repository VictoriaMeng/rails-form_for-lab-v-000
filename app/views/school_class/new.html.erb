<%= form_tag school_class_path, method: "post" do %>
  <label>School Class Title:</label><br>
  <%= text_field_tag :'school_class[title]' %><br>

  <label>School Class Room Number:</label><br>
  <%= number_field_tag :'school_class[room_number]' %><br>

  <%= submit_tag "Submit School Class" %>
<% end %>
